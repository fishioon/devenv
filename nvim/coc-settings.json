{
	"java.format.settings.url": "https://raw.githubusercontent.com/alibaba/p3c/master/p3c-formatter/eclipse-codestyle.xml",
	"java.jdt.ls.vmargs": "-javaagent:/Users/fish/.m2/repository/org/projectlombok/lombok/1.18.6/lombok-1.18.6.jar -Xbootclasspath/a:/Users/fish/.m2/repository/org/projectlombok/lombok/1.18.6/lombok-1.18.6.jar",
	"yank.highlight.enable": false,
	"list.maxHeight": 18,
	"list.autoResize": false,
	"list.normalMappings": {
		"t": "action:tabe",
		"v": "action:vsplit",
		"s": "action:split",
		"d": "expr:GetDeleteAction"
	},
	"list.insertMappings": {
		"<C-t>": "action:tabe",
		"<C-v>": "action:vsplit",
		"<C-w>": "command:wincmd k"
	},
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod"
			],
			"filetypes": [
				"go"
			]
		},
		"clangd": {
			"command": "clangd",
			"args": [
				"--background-index"
			],
			"rootPatterns": [
				"compile_flags.txt",
				"compile_commands.json",
				".vim/",
				".git/",
				".hg/"
			],
			"filetypes": [
				"c",
				"cpp",
				"objc",
				"objcpp"
			]
		}
	}
}
